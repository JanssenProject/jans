<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{dd-MM-yyyy HH:mm:ss.SSS} %-5p %C{4} %F:%L- %m%n" />
        </Console>
    </Appenders>

    <Loggers>
        <Root level="INFO">
			<AppenderRef ref="JANS_LOCK_FILE" />
			<AppenderRef ref="Console" />
        </Root>
		
    </Loggers>

</Configuration>
