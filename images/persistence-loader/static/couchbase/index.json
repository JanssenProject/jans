{
  "!bucket_prefix!_user": {
    "attributes": [
      ["objectClass"], 
      ["creationDate"], 
      ["inum"], 
      ["del","exp"], 
      ["uid"], 
      ["mail"], 
      ["jansStatus"], 
      ["jansApp"], 
      ["jansDeviceHashCode"], 
      ["jansDeviceKeyHandle"],
      ["jansClntId"],
      ["jansUsrId"]
    ],
    "static" : [
      [["LOWER(`uid`)"], "objectClass = \"jansPerson\""], 
      [["LOWER(`mail`)"], "objectClass = \"jansPerson\""], 
      [["exp"], "del=true"] 
    ]
  },

  "!bucket_prefix!": {
    "attributes": [
      ["objectClass"],
      ["jansScrTyp"], 
      ["iat"], 
      ["jansClntSecretExpAt"], 
      ["creationDate"], 
      ["tknCde"], 
      ["inum"], 
      ["displayName"], 
      ["jansRegistrationAccessTkn"], 
      ["jansUmaScope"], 
      ["mail"], 
      ["jansId"], 
      ["jansTicket"], 
      ["del"],
      ["ssnId"],
      ["jansClntId"],
      ["clnId"],
      ["jansUsrDN"],
      ["jansState"],
      ["del","exp"],
      ["objectClass","creationDate","jansStatus"]
    ],
    "static" : [
      [["creationDate"], "objectClass = \"jansPassResetReq\""],
      [["jansScrTyp"], "objectClass = \"jansScr\""],
      [["jansAppTyp", "jansMetricTyp", "jansStartDate", "jansEndDate"], "objectClass = \"jansMetric\""],
      [["exp"], "del=true"],
      [["jansStatus", "exp"], "objectClass = \"jansCibaReq\""]
        
    ]
  },

  "!bucket_prefix!_site": {
    "attributes": [
        ["objectClass"], 
        ["inum"]
    ], 
    "static" : [
    ]
  },

  "!bucket_prefix!_statistic": {
    "attributes": [
      ["objectClass"], 
      ["jansStartDate"], 
      ["del"], 
      ["exp"]
    ],
    "static" : [
    ]
  },

  "!bucket_prefix!_cache": {
    "attributes":[
    ],
    "static" : [
    ]
  },

  "!bucket_prefix!_token": {
    "attributes": [
      ["authzCode"]
    ],
    "static" : [
        [["grtId"], "objectClass = \"jansToken\""],
        [["ssnId"], "objectClass = \"jansToken\""]
    ]
  },

  "!bucket_prefix!_session": {
    "attributes": [
      ["jansUsrDN"]
    ],
    "static" : [
        [["sid"], "objectClass = \"jansSessId\""],
        [["exp"], "objectClass = \"jansSessId\""]
    ]
  },

  "__common__": {
    "attributes":[
    ],
    "static":[
    ]
  }

}
