                <!-- Test: Embedded Jetty -->
                <dependency>
                    <groupId>org.jboss.arquillian.container</groupId>
                    <artifactId>arquillian-jetty-embedded-11</artifactId>
                    <version>${arquillian.jetty.embedded.11.version}</version>
                </dependency>
                <dependency>
                    <groupId>org.eclipse.jetty</groupId>
