    "prettier": "^2.8.8",
    "webpack": "^5.80.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "node-fetch": "^2.6.1",
    "qs": "^6.11.1"
  }
