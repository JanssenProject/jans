JAVA_HOME=/opt/jre
JAVA=$JAVA_HOME/bin/java

OXD_LOGS=/var/log/oxd-server
OXD_HOME=/opt/oxd-https-extension
CONF=$OXD_HOME/lib
LIB=$OXD_HOME/lib

JAVA_OPTIONS="-Xms256m -Xmx512m -XX:MaxMetaspaceSize=256m -XX:+DisableExplicitGC -jar $LIB/oxd-https-extension-jar-with-dependencies.jar server $CONF/oxd-https.yml"

OXD_USER=jetty

