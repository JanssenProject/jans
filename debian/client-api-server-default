JAVA_HOME=/opt/jre
JAVA=$JAVA_HOME/bin/java

JANS_CLIENT_API_LOGS=/var/log/jans-client-api-server
JANS_CLIENT_API_HOME=/opt/jans-client-api-server
CONF=/opt/jans-client-api-server/conf
LIB=$JANS_CLIENT_API_HOME/lib

JAVA_OPTIONS="-server -Xms256m -Xmx512m -XX:MaxMetaspaceSize=256m -XX:+DisableExplicitGC -Djava.net.preferIPv4Stack=true -cp /opt/jans-client-api-server/lib/bcprov-jdk15on-1.54.jar:/opt/jans-client-api-server/lib/jans-client-api-server.jar RpServerApplication server /opt/jans-client-api-server/conf/jans-client-api.yml"

JANS_CLIENT_API_USER=jetty
