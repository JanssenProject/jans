<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:cellview="urn:import:com.google.gwt.user.cellview.client"
        >

    <g:VerticalPanel ui:field="dialogContent" width="500px">

        <cellview:CellTable ui:field="table" width="100%"/>

        <g:HTMLPanel>
            <br/>
        </g:HTMLPanel>
        <g:HTML ui:field="errorMessage" visible="false"/>
        <g:HorizontalPanel>
            <g:Button ui:field="okButton" enabled="false">OK</g:Button>
            <g:Button ui:field="closeButton">Close</g:Button>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>

