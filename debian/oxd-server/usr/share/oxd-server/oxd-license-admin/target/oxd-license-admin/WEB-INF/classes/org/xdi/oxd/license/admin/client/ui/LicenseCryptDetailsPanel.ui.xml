<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:cellview="urn:import:com.google.gwt.user.cellview.client"
        >

    <g:HorizontalPanel ui:field="rootPanel" addStyleNames="detailsPanel" width="100%">
        <g:VerticalPanel width="100%">
            <g:HTML>
                <h2>License ID Details</h2>
            </g:HTML>
            <g:FlowPanel>
                <g:Button ui:field="generateLicenseIdButton" enabled="false">Generate License Ids</g:Button>
                <g:Button ui:field="editButton" enabled="false">Edit (Auto-update)</g:Button>
                <g:Button ui:field="removeButton" enabled="false">Remove</g:Button>
                <g:Button ui:field="refreshButton">Refresh</g:Button>
                <g:Button ui:field="copyIds">Show IDs for copy</g:Button>
            </g:FlowPanel>
            <cellview:CellTable ui:field="licenseIds"/>
        </g:VerticalPanel>
        <g:VerticalPanel width="200px">
            <g:HTML>
                <h3>Name:</h3>
            </g:HTML>
            <g:HTML ui:field="nameField"/>
            <g:HTML>
                <h3>License Id count:</h3>
            </g:HTML>
            <g:HTML ui:field="licenseIdCount"/>
            <g:HTML>
                <h3>Private password:</h3>
            </g:HTML>
            <g:HTML ui:field="privatePassword"/>
            <g:HTML>
                <h3>Public password:</h3>
            </g:HTML>
            <g:HTML ui:field="publicPassword"/>
            <g:HTML>
                <h3>License password:</h3>
            </g:HTML>
            <g:HTML ui:field="licensePassword"/>
            <g:HTML>
                <h3>Private key:</h3>
            </g:HTML>
            <g:HTML ui:field="privateKey"/>
            <g:HTML>
                <h3>Public key:</h3>
            </g:HTML>
            <g:HTML ui:field="publicKey"/>
        </g:VerticalPanel>
    </g:HorizontalPanel>
</ui:UiBinder>
