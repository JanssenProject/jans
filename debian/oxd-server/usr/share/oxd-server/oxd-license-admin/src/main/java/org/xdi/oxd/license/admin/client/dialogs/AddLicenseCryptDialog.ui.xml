<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >

    <g:VerticalPanel ui:field="dialogContent" width="700px">
        <g:HTML>
            <h4>Name:</h4>
        </g:HTML>
        <g:TextBox ui:field="nameField"/>
        <g:HTMLPanel>
            <br/>
        </g:HTMLPanel>
        <g:Button ui:field="generateButton">Generate keys and passwords</g:Button>
        <g:HTML>
            <h4>Private key:</h4>
        </g:HTML>
        <g:HTML ui:field="privateKey"/>
        <g:HTML>
            <h4>Public key:</h4>
        </g:HTML>
        <g:HTML ui:field="publicKey"/>
        <g:HTML>
            <h4>Private password:</h4>
        </g:HTML>
        <g:HTML ui:field="privatePassword"/>
        <g:HTML>
            <h4>Public password:</h4>
        </g:HTML>
        <g:HTML ui:field="publicPassword"/>
        <g:HTML>
            <h4>License password:</h4>
        </g:HTML>
        <g:HTML ui:field="licensePassword"/>

        <g:HTML ui:field="errorMessage" visible="false"/>
        <g:HorizontalPanel>
            <g:Button ui:field="okButton">OK</g:Button>
            <g:Button ui:field="closeButton">Close</g:Button>
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>

