{
  "applicationName":"idp",
  "trustedIdpDn":"ou=trustedIdp,o=jans",
  "enabled": "${idp_enabled}",
  "serverUrl":"https://%(server_url)",
  "realm": "${realm}",
  "clientId": "${client_id}",	
  "clientSecret": "${client_secret}",
  "grantType": "${grant_type}",
  "username": "${user_name}",
  "password": "${password}",  
  "idpRootDir": "${idp_root_dir}",
  "spMetadataRootDir": "${sp_metadata_root_dir}",
  "spMetadataTempDir": "${sp_metadata_temp_dir}",
  "spMetadataFilePattern":"%s-sp-metadata.xml",
  "spMetadataFile":"sp-metadata.xml",
  "ignoreValidation": "${ignore_validation}"
}
