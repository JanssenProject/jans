JAVA_HOME=%(jre_home)s
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS="-server -Xms%(jans-cache-refresh_min_heap_mem)sm -Xmx%(jans-cache-refresh_max_heap_mem)sm -XX:+DisableExplicitGC -Djans.base=%(jansBaseFolder)s -Dserver.base=%(jetty_base)s/jans-cache-refresh -Dlog.base=%(jetty_base)s/jans-cache-refresh -Dpython.home=%(jython_home)s"

JETTY_HOME=%(jetty_home)s
JETTY_BASE=%(jetty_base)s/jans-cache-refresh
JETTY_USER=%(jetty_user)s
JETTY_ARGS="jetty.http.host=localhost jetty.http.port=9091"
TMPDIR=%(jetty_dist)s/temp

export PYTHON_HOME=%(jython_home)s
