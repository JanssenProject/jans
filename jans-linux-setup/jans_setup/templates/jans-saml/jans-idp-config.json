{
  "applicationName":"idp",
  "trustedIdpDn":"ou=trusted-idp,o=jans",
  "realmDn":"ou=realm,o=jans",
  "enabled": "${jans_idp_enabled}",
  "serverUrl":"https://${jans_idp_server_url}",
  "realm": "${jans_idp_realm}",
  "clientId": "${jans_idp_client_id}",
  "clientSecret": "${jans_idp_client_secret}",
  "grantType": "${jans_idp_grant_type}",
  "username": "${jans_idp_user_name}",
  "password": "${jans_idp_password}",  
  "idpRootDir": "${jans_idp_idp_root_dir}",
  "idpMetadataRootDir": "${jans_idp_idp_metadata_root_dir}",
  "idpMetadataTempDir": "${jans_idp_idp_metadata_temp_dir}",
  "idpMetadataFilePattern": "${jans_idp_idp_metadata_file_pattern}",
  "idpMetadataFile": "${jans_idp_idp_metadata_file}",
  "spMetadataUrl":"/realms/%s/broker/%s/endpoint/descriptor",
  "spMetadataRootDir": "${jans_idp_sp_metadata_root_dir}",
  "spMetadataTempDir": "${jans_idp_sp_metadata_temp_dir}",
  "spMetadataFilePattern":"%s-sp-metadata.xml",
  "spMetadataFile":"sp-metadata.xml",
  "ignoreValidation": "${jans_idp_ignore_validation}"
}
