        <classes>
            <class name="io.jans.as.model.authorize.CodeVerifierTest"/>
        </classes>
    </test>

    <test name="JS Engine test" enabled="true">
        <classes>
            <class name="io.jans.as.model.uma.JsonLogicTest" />
        </classes>
    </test>
    <test name="JsonLogic Node Parser Test" enabled="true">
        <classes>
            <class name="io.jans.as.model.uma.JsonLogicNodeParserTest" />
