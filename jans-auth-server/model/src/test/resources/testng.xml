    </test>
    <test name="CertUtils Test" enabled="true">
        <classes>
            <class name="io.jans.as.model.util.CertUtilsTest"/>
        </classes>
    </test>

    <test name="JWK Thumbprint Computation Test" enabled="true">
        <classes>
            <class name="io.jans.as.model.jwk.JSONWebKeyTest"/>
        </classes>
    </test>
