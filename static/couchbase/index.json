{
  "!bucket_prefix!_user": {
    "attributes": [
      ["objectClass"], 
      ["creationDate"], 
      ["inum"], 
      ["del","exp"], 
      ["uid"], 
      ["mail"], 
      ["oxStatus"], 
      ["oxApplication"], 
      ["oxDeviceHashCode"], 
      ["oxDeviceKeyHandle"],
      ["oxAuthClientId"],
      ["oxAuthUserId"]
    ],
    "static" : [
      [["LOWER(`uid`)"], "objectClass = \"gluuPerson\""], 
      [["LOWER(`mail`)"], "objectClass = \"gluuPerson\""], 
      [["exp", "oxAuthExpiration"], "del=true"] 
    ]
  },

  "!bucket_prefix!": {
    "attributes": [
      ["objectClass"],
      ["oxScriptType"], 
      ["oxAuthExpiration"], 
      ["iat"], 
      ["oxAuthClientSecretExpiresAt"], 
      ["creationDate"], 
      ["tknCde"], 
      ["inum"], 
      ["displayName"], 
      ["oxAuthRegistrationAccessToken"], 
      ["oxAuthUmaScope"], 
      ["mail"], 
      ["oxId"], 
      ["oxTicket"], 
      ["del"],
      ["ssnId"],
      ["oxAuthClientId"],
      ["clnId"],
      ["oxAuthSessionId"],
      ["oxAuthUserDN"],
      ["oxState"],
      ["del","exp"],
      ["objectClass","creationDate","oxStatus"]
    ],
    "static" : [
      [["creationDate"], "objectClass = \"gluuPasswordResetRequest\""],
      [["oxScriptType"], "objectClass = \"oxCustomScript\""],
      [["oxApplicationType", "oxMetricType", "oxStartDate", "oxEndDate"], "objectClass = \"oxMetric\""],
      [["exp", "oxAuthExpiration"], "del=true"]
        
    ]
  },

  "!bucket_prefix!_site": {
    "attributes": [
        ["objectClass"], 
        ["inum"]
    ], 
    "static" : [
    ]
  },

  "!bucket_prefix!_statistic": {
    "attributes": [
      ["objectClass"], 
      ["oxStartDate"], 
      ["del"], 
      ["oxAuthExpiration"]
    ],
    "static" : [
    ]
  },

  "!bucket_prefix!_cache": {
    "attributes":[
    ],
    "static" : [
      [["exp"], "objectClass = \"cache\" and del=true"]
    ]
  },

  "!bucket_prefix!_token": {
    "attributes": [
      ["del","exp"],
      ["authzCode"]
    ],
    "static" : [
      [["exp", "oxAuthExpiration"], "del=true"],
      [["grtId"], "objectClass = \"token\""],
      [["ssnId"], "objectClass = \"token\""]
    ]
  },

  "__common__": {
    "attributes":[
    ],
    "static":[
    ]
  }

}
