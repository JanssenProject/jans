JAVA_HOME=/opt/jre
JAVA=$JAVA_HOME/bin/java

OXD_LOGS=/var/log/oxd-server
OXD_HOME=/opt/oxd-https-extension
CONF=$OXD_HOME/lib
LIB=$OXD_HOME/lib
OXD_HTTPS_EXTENSION_VERSION=3.1.2.Final

JAVA_OPTIONS="-server -Xms256m -Xmx512m -XX:MaxMetaspaceSize=256m -XX:+DisableExplicitGC -Doxd.server.config=$CONF/oxd-https.yml -cp $LIB/oxd-https-extension-${OXD_HTTPS_EXTENSION_VERSION}.jar"

OXD_USER=jetty
