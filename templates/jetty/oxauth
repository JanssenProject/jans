JAVA_HOME=%(jre_home)s
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS="-server -Xms256m -XX:MaxMetaspaceSize=%(oxauth_max_mem)sm -XX:+DisableExplicitGC -Dgluu.base=%(gluuBaseFolder)s -Dpython.home=%(jython_home)s -Dcatalina.base=%(jetty_base)s/oxauth"

JETTY_HOME=%(jetty_home)s
JETTY_BASE=%(jetty_base)s/oxauth
JETTY_USER=jetty
JETTY_ARGS="jetty.http.host=localhost jetty.http.port=8081"
TMPDIR=%(jetty_dist)s/temp

export PYTHON_HOME=%(jython_home)s
