JAVA_HOME=%(jre_home)s
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS="-server -Xms256m -XX:MaxMetaspaceSize=%(idp_max_mem)sm -XX:+DisableExplicitGC -Dgluu.base=%(gluuBaseFolder)s -Dcatalina.base=%(jetty_base)s/idp"

JETTY_HOME=%(jetty_home)s
JETTY_BASE=%(jetty_base)s/idp
JETTY_USER=jetty
JETTY_ARGS="jetty.http.host=localhost jetty.http.port=8086"
TMPDIR=%(jetty_dist)s/temp
